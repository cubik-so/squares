{
    "name": "@squares/ui",
    "version": "1.0.0",
    "description": "",
    "main": "lib/index.js",
    "module": "lib/index.mjs",
    "types": "lib/index.d.ts",
    "scripts": {
        "build": "tsup ./src --dts",
        "dev": "pnpm build:fast --watch",
        "clean": "rimraf dist .turbo",
        "type-check": "tsc --noEmit",
        "create:theme": "tsx src/scripts/create-theme.ts",
        "generate:icons": "tsx ./src/scripts/generate-icon.ts",
        "build:fast": "tsup ./src --dts"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "exports": {
        ".": {
            "source": "./src/index.ts",
            "import": {
                "types": "./lib/index.d.ts",
                "default": "./lib/index.mjs"
            },
            "require": {
                "types": "./lib/index.d.ts",
                "default": "./lib/index.js"
            }
        },
        "./styles.css": "./lib/styles/index.css",
        "./theme": {
            "import": "./lib/config/theme.mjs",
            "require": "./lib/config/theme.js",
            "types": "./lib/config/theme.d.ts"
        },
        "./icons": {
            "import": "./lib/icons.mjs",
            "require": "./lib/icons.js",
            "types": "./lib/icons.d.ts"
        }
    },
    "devDependencies": {
        "@squares/configs": "workspace:*",
        "@types/node": "^20.11.29",
        "@types/react": "^18.2.67",
        "@types/react-dom": "^18.2.22",
        "@vitejs/plugin-react": "^4.2.1",
        "postcss": "^8.4.36",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "tailwind-merge": "^2.2.2",
        "tailwindcss": "^3.4.1",
        "tailwindcss-animate": "^1.0.7",
        "tsx": "^4.7.1",
        "typescript": "^5.2.2"
    },
    "dependencies": {
        "@radix-ui/react-dialog": "^1.0.5",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.1.0",
        "framer-motion": "^11.0.20",
        "ts-node": "^10.9.2",
        "tsup": "^8.0.2"
    }
}
